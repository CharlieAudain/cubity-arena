# Development Environment Variables
# Copy this file to .env for local development

# WebSocket Server URL (leave as localhost for local dev)
VITE_SOCKET_URL=http://localhost:3001

# Firebase Configuration (from Firebase Console)
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Admin Emails (comma-separated list of admin user emails)
# Users with these emails will see debug features and admin tools
VITE_ADMIN_EMAILS=your-email@gmail.com,another-admin@example.com
